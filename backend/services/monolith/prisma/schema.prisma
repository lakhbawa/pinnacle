generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Board {
  id        String   @id @default(uuid()) @map("uuid")
  title     String
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@map("board")
}
